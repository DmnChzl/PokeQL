  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/mongodb": "^3.3.12",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.16",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
