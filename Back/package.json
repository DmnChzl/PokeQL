{
  "name": "pokeql-back",
  "version": "1.1.0",
  "description": "Back-End For PokeQL App Made With ExpressJS, Mongoose & GraphQL",
  "author": {
    "name": "Damien Chazoule",
    "email": "damien.chazoule@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrdoomy/pokeql.git"
  },
  "scripts": {
    "build": "rimraf build/ && tsc && cp -r public build/",
    "start": "ts-node ./src/main.ts",
