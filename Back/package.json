    "graphql": "^14.5.7",
    "graphql-tools": "^4.0.5",
    "merge-graphql-schemas": "^1.7.0",
    "mongoose": "^5.7.5",
    "nodemon": "^1.19.2",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/mongodb": "^3.3.3",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.7.7",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "license": "Beerware",
  "keywords": [
